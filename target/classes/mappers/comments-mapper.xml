<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="commentsMapper">

 	<resultMap id="commentsResultMap" type="Comments"></resultMap>
 	
	<select id="selectComments" resultMap="commentsResultMap" parameterType="_int" resultType="Comments">
		SELECT *
		FROM COMMENTS
		WHERE BCODE=#{bcode}
	</select>
	
</mapper>
